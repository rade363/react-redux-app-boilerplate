{
  "name": "react-app-boilerplate",
  "version": "1.0.0",
  "description": "React app boilerplate",
  "main": "index.js",
  "scripts": {
    "check:code": "tsc --noEmit",
    "lint:code": "eslint ./src --ext .ts,.tsx",
    "lint:codefix": "eslint ./src --ext .ts,.tsx --fix",
    "lint:css": "stylelint src/client/css/*.scss",
    "lint:cssfix": "stylelint src/client/css/*.scss --fix",
    "clean": "rimraf dist",
    "build:client": "webpack --config webpack/client.config.ts",
    "build:server": "webpack --config webpack/server.config.ts",
    "build": "npm-run-all --print-label build:*",
    "test": "jest",
    "start": "node index.js"
  },
  "author": "Ilya Belyavskiy",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rade363/react-app-boilerplate.git"
  },
  "engines": {
    "node": ">=12"
  },
  "bugs": {
    "url": "https://github.com/rade363/react-app-boilerplate/issues"
  },
  "homepage": "https://github.com/rade363/react-app-boilerplate#readme",
  "dependencies": {
    "axios": "0.24.0",
    "cross-env": "7.0.3",
    "dotenv": "10.0.0",
    "express": "4.17.1",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@babel/core": "7.16.0",
    "@babel/plugin-proposal-object-rest-spread": "7.16.0",
    "@babel/preset-env": "7.16.4",
    "@babel/preset-react": "7.16.0",
    "@babel/preset-typescript": "7.16.0",
    "@babel/register": "7.16.0",
    "@testing-library/jest-dom": "5.15.1",
    "@testing-library/react": "12.1.2",
    "@types/express": "4.17.13",
    "@types/jest": "27.0.3",
    "@types/mini-css-extract-plugin": "2.4.0",
    "@types/node": "16.11.10",
    "@types/react": "17.0.37",
    "@types/react-dom": "17.0.11",
    "@types/react-hot-loader": "4.1.1",
    "@types/webpack-dev-middleware": "5.0.2",
    "@types/webpack-hot-middleware": "2.25.5",
    "@types/webpack-node-externals": "2.5.3",
    "@typescript-eslint/eslint-plugin": "5.4.0",
    "@typescript-eslint/parser": "5.4.0",
    "babel-jest": "27.3.1",
    "babel-plugin-transform-class-properties": "6.24.1",
    "css-loader": "6.5.1",
    "eslint": "8.3.0",
    "eslint-config-airbnb": "19.0.1",
    "eslint-config-airbnb-typescript": "16.0.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-react": "7.27.1",
    "eslint-plugin-react-hooks": "4.3.0",
    "html-webpack-plugin": "5.5.0",
    "jest": "27.3.1",
    "jest-fetch-mock": "3.0.3",
    "mini-css-extract-plugin": "2.4.5",
    "npm-run-all": "4.1.5",
    "react-hot-loader": "4.13.0",
    "react-test-renderer": "17.0.2",
    "sass": "1.43.5",
    "sass-loader": "12.3.0",
    "style-loader": "3.3.1",
    "stylelint": "14.1.0",
    "stylelint-config-standard": "24.0.0",
    "stylelint-config-standard-scss": "3.0.0",
    "ts-loader": "9.2.6",
    "tsconfig-paths-webpack-plugin": "3.5.2",
    "typescript": "4.5.2",
    "webpack": "5.64.4",
    "webpack-cli": "4.9.1",
    "webpack-dev-middleware": "5.2.2",
    "webpack-dev-server": "4.5.0",
    "webpack-hot-middleware": "2.25.1",
    "webpack-node-externals": "^3.0.0"
  }
}
